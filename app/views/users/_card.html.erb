

<% if member.team.present? %>
<div class="cardtrips">

  <%# if member.team.end_date.to_date > Date.today %>
  <%# else %>
  <%# end %>

  <%# if member.team.team_picture.present? %>
  <%#= cl_image_tag member.team.team_picture, class: "picture-card" %>
  <%# else %>
  <%#= cl_image_tag("default_fkhyps.png", class: "picture-card") %>
  <%# end %>
  <% if policy(member.team).show? %>
  <h6><%= member.team.city.name %></h6>
  From <%= member.team.start_date.strftime("%d")%> to
  <%= member.team.end_date.strftime("%d %B") %>
  <%#= member.team.score %>

  <%= link_to "", team_path(member.team.id), class: "card-link" %>
  <% end %>

  <%# if member.team.end_date.to_date > Date.today %>
  <%# else %>
  <%# end %>
</div>
<div class="deletecard text-center">
 <% if policy(member.team).destroy? %>
 <%= link_to "DELETE", team_path(member.team), method: :delete %>
 <% end %>
</div>
<% end %>
