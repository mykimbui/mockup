<div class="row">
  <div class="col-xs-3">
    <%= cl_image_tag team_challenge.challenge.picture %>
  </div>
  <div class="col-xs-9">
    <h2><%= team_challenge.challenge.title %></h2>
    <%= link_to "", team_challenge_path(@team.id, team_challenge.challenge.id) %>
  </div>
</div>

<% if !team_challenge.answer.nil? %>
<% if team_challenge.answer.status == "completed" && team_challenge.challenge.gift == "" %>
<% elsif team_challenge.answer.status == "completed" && team_challenge.challenge.gift != "" %>
<% end %>
<% end %>

